{
  "name": "twa-solidjs-ui",
  "version": "0.0.1",
  "description": "library of ui components for telegram web app",
  "license": "MIT",
  "author": "Soullnik",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soullnik/twa-solidjs-ui.git"
  },
  "homepage": "https://github.com/Soullnik/twa-solidjs-ui#readme",
  "bugs": {
    "url": "https://github.com/Soullnik/twa-solidjs-ui/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "@solidjs/router": "^0.8.3",
    "@tma.js/sdk": "^1.0.0",
    "@tma.js/sdk-solid": "^1.0.0",
    "@tma.js/solid-router-integration": "^1.0.0",
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.11",
    "esbuild-plugin-solid": "^0.5.0",
    "esbuild-sass-plugin": "^2.16.1",
    "eslint": "^8.56.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^23.0.1",
    "prettier": "3.1.1",
    "sass": "^1.69.7",
    "tsup": "^8.0.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-sass-dts": "^1.3.17",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.1.2"
  },
  "keywords": [
    "solid",
    "twa",
    "telegram"
  ],
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  },
  "dependencies": {
    "@solid-primitives/scheduled": "^1.4.1",
    "lottie-web": "^5.12.2",
    "solid-icons": "^1.1.0",
    "solid-transition-group": "^0.2.3",
    "swiper": "^11.0.5"
  }
}
